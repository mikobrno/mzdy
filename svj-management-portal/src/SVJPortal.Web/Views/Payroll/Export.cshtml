@model IEnumerable<SVJPortal.Web.Models.Payroll>

<h2>Export mezd</h2>

<p>
    <a href="/Payroll/ExportCsv" class="btn btn-primary">Exportovat do CSV</a>
    <a href="/Payroll/ExportPdf" class="btn btn-secondary">Exportovat do PDF</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>Rok</th>
            <th>Měsíc</th>
            <th>Hrubá mzda</th>
            <th>Čistá mzda</th>
            <th>Stav</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var payroll in Model)
        {
            <tr>
                <td>@payroll.Rok</td>
                <td>@payroll.NazevMesice</td>
                <td>@payroll.HrubaMzda</td>
                <td>@payroll.CistaMzda</td>
                <td>@payroll.Stav</td>
            </tr>
        }
    </tbody>
</table>
